<h1>Stores</h1>
<p>Find me in app/views/search/index.html.erb</p>
<div id="total">
  <h3><%= @total %> total stores </h3>
</div>

<div id="stores">
  <table>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>Distance</th>
      <th>Phone</th>
    </tr>
  <% @stores.each do |store| %>
  <div class="store">
    <tr>
      <td>store.name</td>
      <td>store.city</td>
      <td>store.distance</td>
      <td>store.phone</td>
    </tr>
    </table>

  </div>
  <% end %>


</div>
